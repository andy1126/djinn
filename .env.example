# Djinn 环境变量配置
# 复制此文件为 .env 并填写实际值

# 数据源配置
# Tushare token (如果需要使用Tushare数据源)
TUSHARE_TOKEN=your_tushare_token_here

# 缓存配置
CACHE_DIR=./.cache/djinn
CACHE_TTL=3600  # 缓存过期时间（秒）

# Redis配置（可选，用于分布式缓存）
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=./logs/djinn.log

# 性能配置
MAX_WORKERS=4  # 最大并行工作线程数
CHUNK_SIZE=1000  # 数据分块大小

# 代理配置（如果需要）
HTTP_PROXY=http://proxy.example.com:8080
HTTPS_PROXY=http://proxy.example.com:8080

# 开发配置
DEBUG=false
TEST_MODE=false